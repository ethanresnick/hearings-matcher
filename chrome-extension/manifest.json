{
  "manifest_version": 2,
  "name": "Hearings Matcher",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "html": ["index.html"],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
    "background": {
    "scripts": ["background.js"]
  }
}